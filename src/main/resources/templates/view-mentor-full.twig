{% extends "classpath:/templates/index-admin.twig" %}



{% block content %}
    <div class="table-container">
        <table class="table">
            <thead>
            <tr>
                <th>#</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>e-mail</th>
            </tr>
            </thead>
            <tbody>
            {% set ID = 1 %}
                {% for element in data %}
                    <tr>
                        <td>{{ID}}</td>
                        {% for record in element %}
                        <td>{{ record }}</td>
                        {% endfor %}
                    <tr>
                    {% set ID = ID + 1 %}
                {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}