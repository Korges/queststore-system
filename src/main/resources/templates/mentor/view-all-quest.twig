{% extends "classpath:/templates/mentor/nav.twig" %}

{% block content %}
    <div class="title">View questr</div>
        <table class="table">
            <thead>
            <tr>
                <th>Quest ID</th>
                <th>Name</th>
                <th>Description</th>
                <th>Value</th>
                <th>Experience</th>
                <th>Category</th>
            </tr>
            </thead>
            <tbody>
                {% for element in data %}
                    <tr>
                        {% for record in element %}
                        <td>{{ record }}</td>
                        {% endfor %}
                    </tr>
                {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}