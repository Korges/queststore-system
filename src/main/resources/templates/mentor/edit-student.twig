{% extends "classpath:/templates/mentor/nav.twig" %}

{% block content %}
    <div class="main-container">
        <div class="header-container">
            <h1>Edit Student</h1>
        </div>
        <div class="container">
            <form>
                <fieldset>
                    <legend>Legend:</legend>
                    Students Id:<br>
                    <input type="number" name="id" autofocus="autofocus"><br>
                    Students Name:<br>
                    <input type="text" name="name"><br>
                    Students Last Name:<br>
                    <input type="text" name="last-name" ><br>
                    Students email:<br>
                    <input type="email" name="email"><br>
                    Students password:<br>
                    <input type="password" name="password"><br>
                    <input type="submit" value="Submit">
                    <input type="reset">
                </fieldset>
            </form>
        </div>
    </div>
{% endblock %}
